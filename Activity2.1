import turtle as t

t.pensize(3)

t.pencolor("blue")
t.fillcolor("green")
t.begin_fill()
t.circle(10)
t.end_fill()
t.forward(25)
t.right(90)
t.forward(50)
t.right(90)
t.forward(25)
t.fillcolor("red")
t.begin_fill()
t.circle(10)
t.end_fill()
t.forward(25)
t.right(90)
t.forward(50)
t.right(90)
t.forward(25)
t.hideturtle()
